<section>
  <div class="content-wrapper">
    <div class="content-heading">
      <%- include('../../includes/language.ejs') -%>
        <span data-localize="new.ADD.HEAD"></span>
    </div>
    <div class="row">
      <div class="col-sm-12">
        <div class="panel panel-default">
          <div class="panel-body">
            <form role="form" id="adminForm">
              <div class="form-group">
                <label data-localize="new.ADD.NAME">User Name</label>
                <input type="text" placeholder="User Name" id="username" class="form-control">
              </div>
              <div class="form-group">
                <label data-localize="new.ADD.EMAIL">Email</label>
                <input type="email" placeholder="Email" id="email" class="form-control">
              </div>
              <div class="form-group">
                <label data-localize="new.ADD.PASSWORD">Password</label>
                <input type="password" placeholder="Password" id="password" class="form-control">
              </div>
              <div class="form-group">
                <label data-localize="new.ADD.CONFIRM-PASSWORD">Confirm Password</label>
                <input type="password" placeholder="Confirm Password" id="cpassword" class="form-control">
              </div>
              <button type="submit" class="btn btn-sm btn-info" data-localize="new.ADD.BUTTON">Add</button>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
