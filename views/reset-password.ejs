<div class="main_content">
  <div class="searching_container">
    <div class="reglogin_container">
      <form id="change_password" class="form_login_id">
        <h1 class="reglogin_title">Change Password</h1>
        <input type="hidden" id="userToken" value="<%= token%>"/>
        <div class="reglogin_main_info" style="border-top:  none; padding-bottom: 0;">
          <div class="row">
            <div class="searching-lg-6 searching-md-6 searching-sm-6 searching-xs-12">
              <label for="loginform-email" class="reglogin_input_label viewform_new_label" >New Password</label>
              <div class="input_wrap">
                <div class="form-group field-loginform-email required">
                  <input type="password" id="reset-newpassword" class="" name="LoginForm[newpass]" autofocus placeholder="New Password" aria-required="true">
                  <p class="help-block help-block-error"></p>
                </div>
              </div>
            </div>
            <div class="searching-lg-6 searching-md-6 searching-sm-6 searching-xs-12">
              <label for="loginform-password" class="reglogin_input_label viewform_confirm_label">Password</label>
              <div class="input_wrap">
                <div class="form-group field-loginform-password required">
                  <input type="password" id="reset-cpassword" class="pass" name="LoginForm[cpass]" placeholder="Confirm Password" aria-required="true">
                  <p class="help-block help-block-error"></p>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="text-center reglogin_but">
          <button type="submit" class="enter_btn">Change</button>
        </div>
      </form>
    </div>
  </div>
</div>
